using namespace std;

int dp[200001];
int m;

int main(int argc, const char * argv[]){
    cin >> m;
    
    int n;
    cin >> n;
    
    dp[0] = 0;
    for(int i = 1; i <= m; i++)
        dp[i] = 300000000;
    
    for(int i = 0; i < n; i++){
        int q, r; // q人 r円
        cin >> q >> r;
        
        for(int j = m; j > q; j--){
            int c = dp[j-q] + r;
            if(c < dp[j]) dp[j] = c;
        }
        
